programa {
  funcao inicio() {
    
    inteiro numero1, numero2

    escreva("digite o primeiro número: ")
    leia(numero1)

    escreva("digite o segundo número: ")
    leia(numero2)

    se (numero1 > numero2){
    escreva("o número ", numero1, " é maior que o ", numero2)
    }
    senao {
      escreva("o número ", numero1, "é menor que o ", numero2)
    }
    

  }
}