programa {
  inteiro numero, numero2
  funcao inicio() {
    escreva("digite o primeiro número: ")
    leia(numero)

    escreva("digite o segundo número: ")
    leia(numero2)

    escreva("a soma dos números é: ", Somar(numero, numero2))
    
      }


  funcao inteiro Somar(inteiro n1, inteiro n2){
    retorne n1 + n2
  }
}